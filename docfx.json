{
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}",
          "**/*.yml",
          "xapi/toc.yml",
          "**/images/**"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "mylearnerdata",
      "_appTitle": "mylearnerdata",
      "_appLogoUrl": "https://mylearnerdata.com",      
      "_appLogoPath": "images/custom.svg",
      "_appFaviconPath": "images/custom.ico",
      "_appFooter":"<a href=\"https://deltlabs.com\">DeLT Labs GmbH</a> <a href=\"/imprint.html\">Impressum</a>",      
      "_disableContribution": true,
      "_disableBreadcrumb": true,
      "_enableSearch": true,
      "pdf": false
    }
  }
}